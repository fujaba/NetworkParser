<?xml version="1.0" encoding="UTF-8" standalone="no"?><!DOCTYPE project>
<project default="ikvm" name="Create DLL for NetworkParser">
	<property name="ikvmVersion" value="7.4.5148" />
	<import file="buildAnt.xml" as="Ant"/>
	<target name="unpack">
		<unzip src="lib/ikvm-8.0.5305.zip" dest="build/ikvm" />
	</target>
	<target name="ikvm" depends="unpack, Ant.buildAnt">
		<taskdef name="ikvmc" classname="net.ikvm.ant.IkvmcTask" classpath="lib/ant-ikvmc.0.5.jar" />
		<echo>${basedir}/build/ikvm/</echo>
		<ikvmc target="library" out="${basedir}/build/networkparser.dll" home="${basedir}\build\ikvm\" version="4.2.23.${Revisionnumber}" debug="true">
			<fileset dir="./bin" excludes="de/uniks/networkparser/test/**" />
		</ikvmc>
		<copy file="${basedir}/build/ikvm/bin/IKVM.OpenJDK.Core.dll" tofile="build/IKVM.OpenJDK.Core.dll" />
		<copy file="${basedir}/build/ikvm/bin/IKVM.OpenJDK.Util.dll" tofile="build/IKVM.OpenJDK.Util.dll" />
		<copy file="${basedir}/build/ikvm/bin/IKVM.Runtime.dll" tofile="build/IKVM.Runtime.dll" />
		<delete dir="build/temp"></delete>
	</target>
</project>